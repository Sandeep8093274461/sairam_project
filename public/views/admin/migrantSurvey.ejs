<%- include('common/header.ejs') -%>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <style>
                .form-control {
                    max-width: 400px;
                }
        
                .row {
                    margin-bottom: 10px;
                }
            </style>
            <!-- Content Header (Page header) -->
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-1">
                        <div class="col-8">
                            <h1 class="m-0 text-dark"><img src="/img/icon/logo.jpg" style="width:60px;height:60px;"
                                    alt=""> Survey for promotion of  Sai Swabalambi</h1>
                        </div><!-- /.col -->
                        <div class="col-4">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#"> Sai Swabalambi Program</a></li>
                                <li class="breadcrumb-item active">Skill</li>
                                <li class="breadcrumb-item active">Entry</li>
                            </ol>
                        </div><!-- /.col -->
                    </div><!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content-header -->
            <!-- Main content -->



            <div class="container-fluid" ng-show="showAppForm">
                <div class="container-fluid">
                    <div class="panel panel-default">

                        <div class="card card-primary" ng-show="part1">
                            <div class="card-header">
                                <h3 class="card-title">Personal Details</h3>
                            </div>
                            <form name="valid1" id="valid1">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Full name <span class="star">*</span> :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="migrantName"
                                                        ng-model="migrantName" placeholder="Enter full name"
                                                        required>
                                                    <span style="color:red"
                                                        ng-show="valid1.migrantName.$dirty && valid1.migrantName.$invalid">
                                                        <span ng-show="valid1.migrantName.$error.required">Name is
                                                            Required .</span>
                                                    </span>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <label>Date of birth<span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <input type="date" class="form-control" name="dob" ng-model="dob"
                                                    placeholder="Enter Your Date of Birth" required>
                                            </div>

                                        </div>
                                        <div class="col-4">
                                            <label>Gender<span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" name="gender" ng-model="gender" required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Male"> Male</option>
                                                    <option value="Female"> Female</option>
                                                    <option value="Transgender"> Transgender</option>
                                                </select>
                                            </div>


                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Contact number <span class="star">*</span> :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="mobNo"
                                                        ng-model="mobNo" placeholder="Enter contact number"
                                                        maxlength="10" minlength="10" ng-pattern="/^[1-9][0-9]{9}$/"
                                                        allow-numbers-only onkeypress='numberValidate(event)' required>
                                                    <span style="color:red"
                                                        ng-show="valid1.mobNo.$dirty && valid1.mobNo.$invalid">
                                                        <span ng-show="valid1.mobNo.$error.required">Mobile No is
                                                            Required .</span>
                                                    </span>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <label for="Phone">Category <span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="category" name="category"
                                                    required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="ST"> ST</option>
                                                    <option value="SC"> SC</option>
                                                    <option value="OBC"> OBC</option>
                                                    <option value="Other"> Other</option>
                                                </select>
                                            </div>


                                            </span>
                                        </div>
                                        <div class="col-4">
                                            <label for="Phone">Marital status <span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="material" name="material"
                                                    required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Married"> Married</option>
                                                    <option value="Unmarried"> Unmarried</option>
                                                    <option value="Widow"> Widow</option>
                                                </select>
                                            </div>

                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Father/Guardian name<span class="star">*</span> :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="fatherName"
                                                        ng-model="fatherName"
                                                        placeholder="Enter Father/Guardian name" required>

                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Total family members <span class="star">*</span> :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="fMember"
                                                        ng-model="fMember" placeholder="Enter total family member "
                                                        allow-numbers-only onkeypress='numberValidate(event)' required>
                                                    <span style="color:red"
                                                        ng-show="valid1.fMember.$dirty && valid1.fMember.$invalid">
                                                        <span ng-show="valid1.fMember.$error.required">Total Family
                                                            Member is
                                                            Required .</span>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4" ng-if="district_id === ''">
                                            <label for="Phone"> District<span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="$parent.district" name="district" ng-change="loadblockList()" required>
                                                    <option value="" disabled>--District--</option>
                                                    <option ng-repeat="option in districtList" value="{{option.district_id}}" >{{option.name}}</option>
                                                </select>

                                            </div>

                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Block/Municipality name <span class="star">*</span> :</label>
                                                <div class="col-md-12">
                                                        <select class="form-control" ng-model="block" name="block"
                                                        ng-change="loadgp()" required>
                                                        <option value="" disabled selected>--select--</option>
                                                        <option ng-repeat="option in blockList" value="{{option.block_code}}" >{{option.block_name}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>GP/Ward name <span class="star">*</span> :</label>
                                                <div class="col-md-12">
                                                        <div class="col-md-12">
                                                            <select class="form-control" ng-model="gp" name="gp" ng-change="loadVillages();" required>
                                                            <option value="" disabled selected>--select--</option>
                                                            <option ng-repeat="option in gpList" value="{{option.panchayat_code}}" >{{option.panchayat_name}}</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Village/Area name<span class="star">*</span> :</label>
                                                <div class="col-md-12">
                                                    <select class="form-control" ng-model="village" name="village" required>
                                                        <option value="" disabled selected>--select--</option>
                                                        <option ng-repeat="option in villageList" value="{{option.villagecode}}" >{{option.villagename}}</option>
                                                    </select>
                                                    

                                                </div>
                                            </div>
                                        </div>
                                        <br>


                                    </div>
                                    <div class="row">
                                        <div class="col-md-6" style="text-align: left;" ng-show="migrate_id == '' ">
                                           
                                        </div>
                                        <div class="col-md-6" style="text-align: left;" ng-hide="migrate_id == '' ">
                                            <button class="btn btn-success " ng-click="previous5();"
                                            style="text-align: right; "><i class="fa fa-arrow-left" aria-hidden="true"></i> Back</button>
                                        </div>

                                        <div class="col-md-6" style="text-align: right;">
                                            <button type="button" class="btn btn-success" ng-click="next1();"ng-disabled="valid1.$invalid" >
                                                Next <i class="fa fa-arrow-right" aria-hidden="true"></i></button>
                                            <!-- ng-disabled="valid1.$invalid" -->
                                        </div>
                                    </div>
                                </div>
                            </form>

                        </div>
                        <div class="card card-primary" ng-show="part2">
                            <div class="card-header">
                                <h3 class="card-title"> Present income status and qualification details </h3>
                            </div>
                            <form name="valid2" id="valid2">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Farmer id <span class="star">*</span>:</label>
                                                <div class="col-md-12">
                                                    <select class="form-control" ng-model="farmerId" name="farmerId"
                                                        required>
                                                        <option value="" disabled selected>--select--</option>
                                                        <option value="Yes"> Yes</option>
                                                        <option value="No"> No</option>
                                                    </select>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <label for="Phone">Bank account<span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="acNumber" name="acNumber"
                                                    required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Yes"> Yes</option>
                                                    <option value="No"> No</option>
                                                </select>
                                            </div>

                                        </div>

                                        <div class="col-4">
                                            <label for="Phone">Aadhaar number<span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="aadharNumber" name="aadharNumber"
                                                    required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Yes"> Yes</option>
                                                    <option value="No"> No</option>
                                                </select>
                                            </div>

                                        </div>

                                        <div class="col-4">
                                            <label for="Phone">Voter-id<span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="voterId" name="voterId" required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Yes"> Yes</option>
                                                    <option value="No"> No</option>
                                                </select>
                                            </div>

                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Total land
                                                    area of
                                                    family in
                                                    acre<span class="star">*</span>
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="landArea"
                                                        ng-model="landArea"
                                                        placeholder="Enter Total land area" required>
                                                    <span style="color:red"
                                                        ng-show="valid2.landArea.$dirty && valid2.landArea.$invalid">
                                                        <span ng-show="valid2.landArea.$error.required">Total Land
                                                            area is
                                                            Required .</span>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Present source of income<span class="star">*</span>
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="salary"
                                                        ng-model="salary"
                                                        placeholder="Enter present source of income" required>
                                                    <span style="color:red"
                                                        ng-show="valid2.salary.$dirty && valid2.salary.$invalid">
                                                        <span ng-show="valid2.salary.$error.required">Present source
                                                            of Income is
                                                            Required .</span>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Monthly
                                                    income at
                                                    present<span class="star">*</span>
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="monthlyIncome"
                                                        ng-model="monthlyIncome"
                                                        placeholder="Monthly income in Rs" maxlength="7"
                                                        minlength="1" onkeypress='numberValidate(event)'
                                                        allow-numbers-only required>
                                                    <span style="color:red"
                                                        ng-show="valid2.monthlyIncome.$dirty && valid2.monthlyIncome.$invalid">
                                                        <span ng-show="valid2.monthlyIncome.$error.required">Monthly
                                                            income is
                                                            Required .</span>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <label for="Phone">Highest education qualification <span
                                                    class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="qualification"
                                                    name="qualification" required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="ClassI"> Class I</option>
                                                    <option value="ClassII"> Class II</option>
                                                    <option value="ClassIII"> Class III</option>
                                                    <option value="ClassIV"> Class IV</option>
                                                    <option value="ClassV"> Class V</option>
                                                    <option value="ClassVI"> Class VI</option>
                                                    <option value="ClassVII"> Class VII</option>
                                                    <option value="ClassVIII"> Class VIII</option>
                                                    <option value="ClassIX"> Class IX</option>
                                                    <option value="ClassX"> Class X</option>
                                                    <option value="ClassXI"> Class XI</option>
                                                    <option value="ClassXII"> Class XII</option>
                                                    <option value="BachelorsDegree"> Bachelors Degree</option>
                                                    <option value="MastersDegree"> Masters Degree</option>
                                                    <option value="Other"> Other</option>
                                                </select>
                                            </div>

                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Skill development certification
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control"
                                                        name="skillDevelopmentCertification"
                                                        ng-model="skillDevelopmentCertification"
                                                        placeholder="Enter skill development certification name">
                                                    <span style="color:red"
                                                        ng-show="valid2.skillDevelopmentCertification.$dirty && valid2.skillDevelopmentCertification.$invalid">
                                                        <span
                                                            ng-show="valid2.skillDevelopmentCertification.$error.required">Skill
                                                            Development Certification Name is
                                                            Required .</span>
                                                    </span>

                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>Are you Migrants? <span class="star">*</span></label>
                                                <div class="col-md-12">
                                                    <select class="form-control" ng-model="migrant" name="migrant"
                                                        required>
                                                        <option value="" disabled selected>--select--</option>
                                                        <option value="Yes"> Yes</option>
                                                        <option value="No"> No</option>
                                                    </select>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <label for="Phone">Animals present in the family<span
                                                    class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="animals" name="animals" required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Yes"> Yes</option>
                                                    <option value="No"> No</option>
                                                </select>
                                            </div>
                                            <br>

                                        </div>
                                        
                                        <div class="col-4" ng-if="animals == 'Yes'">
                                            <div class="form-group">
                                                <label>Cow +
                                                    Bullocks
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="noOfCows"
                                                        ng-model="$parent.noOfCows"
                                                        placeholder="Enter no of cows & bullocks" maxlength="2"
                                                        minlength="1" onkeypress='numberValidate(event)'
                                                        allow-numbers-only required>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4" ng-if="animals == 'Yes'">
                                            <div class="form-group">
                                                <label>Goat
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="noOfGoat"
                                                        ng-model="$parent.noOfGoat" placeholder="Enter no of goat"
                                                        maxlength="2" minlength="1" onkeypress='numberValidate(event)'
                                                        allow-numbers-only required>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4" ng-if="animals == 'Yes'">
                                            <div class="form-group">
                                                <label>Sheep
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="noOfSheep"
                                                        ng-model="$parent.noOfSheep" placeholder="Enter no of sheep"
                                                        maxlength="2" minlength="1" onkeypress='numberValidate(event)'
                                                        allow-numbers-only required>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4" ng-if="animals == 'Yes'">
                                            <div class="form-group">
                                                <label>Others
                                                    if any Animals
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="noOfOtherAnimal"
                                                        ng-model="$parent.noOfOtherAnimal"
                                                        placeholder="Enter no of other animal" maxlength="2"
                                                        minlength="1" onkeypress='numberValidate(event)'
                                                        allow-numbers-only required>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6" style="text-align: left;">
                                            <button class="btn btn-success " ng-click="previous1();"
                                                style="text-align: right;"><i class="fa fa-arrow-left" aria-hidden="true"></i> Back</button>
                                        </div>

                                        <div class="col-md-6" style="text-align: right;">
                                            <button type="button" class="btn btn-success" ng-click="next2();"ng-disabled="valid2.$invalid"  >
                                                Next <i class="fa fa-arrow-right" aria-hidden="true"></i></button>
                                            <!-- ng-disabled="valid2.$invalid" -->
                                        </div>
                                    </div>


                                </div>
                            </form>

                        </div>
                        <div class="card card-primary" ng-show="part3 " ng-if="migrant === 'Yes'">
                            <div class="card-header">
                                <h3 class="card-title">Migration details</h3>
                            </div>
                            <form name="valid3" id="valid3">
                                <div class="card-body"><br>
                                    <div class="row">
                                        <br>
                                        <div class="col-4">
                                            <label for="Phone">Coming from <span class="star">*</span>:</label>
                                            <div class="col-md-12">
                                                <select class="form-control" ng-model="$parent.returningState" name="returningState" required>
                                                    <option value="" disabled>--District--</option>
                                                    <option ng-repeat="option in states" value="{{option.statecode}}" >{{option.statename}}</option>
                                                </select>

                                            </div>
                                            

                                        </div>

                                        <div class="col-4" style="margin-top: -23px;">
                                            <div class="col-md-12">
                                                <label for="Phone">Do you plan to go back to previous
                                                    work place?<span class="star">*</span>:</label>
                                            </div>

                                            <div class="col-md-12">
                                                <select class="form-control" name="planAfterLockDown"
                                                    ng-model="$parent.planAfterLockDown" required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Immediately after lock down"> Immediately after
                                                        lock down</option>
                                                    <option value="Within three months">Within three months</option>
                                                    <option value="After 3-6 months">After 3-6 months</option>
                                                    <option value="After 7-12 months"> After 7-12 months</option>
                                                    <option value="Don’t Know">Don’t Know</option>
                                                    <option value="Never">Never</option>
                                                </select>
                                            </div>

                                        </div>
                                        <div class="col-4" style="margin-top: -23px;">
                                            <div class="col-md-12">
                                                <label for="Phone">If you stay back, how do you plan for
                                                    earning?<span class="star">*</span>:</label>
                                            </div>

                                            <div class="col-md-12">
                                                <select class="form-control" name="stayBackAfterLockDown"
                                                    ng-model="$parent.stayBackAfterLockDown" required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Have some savings/resources"> Have some
                                                        savings/resources</option>
                                                    <option value="Will look for employment locally">Will look for
                                                        employment locally</option>
                                                    <option value="Will become self-employed">Will become
                                                        self-employed</option>
                                                    <option value="Will return to agriculture"> Will return to
                                                        agriculture</option>
                                                    <option value="Will take loan from friends/relatives/other">
                                                        Will take loan from friends/relatives/other</option>
                                                    <option value="Hope government will support">Hope government
                                                        will support</option>
                                                    <option value="Others">Others</option>
                                                    <option value="Don’t Know">Don’t Know</option>
                                                </select>
                                            </div>
                                            <br>

                                        </div>
                                        <div class="col-4">
                                            <div class="form-group">
                                                <label>What is your
                                                    expected income?<span class="star">*</span>
                                                    :</label>
                                                <div class="col-md-12">
                                                    <input type="text" class="form-control" name="expectedSalary"
                                                        ng-model="$parent.expectedSalary"
                                                        placeholder="Enter expected income per Month"
                                                        maxlength="7" minlength="1" onkeypress='numberValidate(event)'
                                                        allow-numbers-only required>
                                                    <span style="color:red"
                                                        ng-show="valid3.expectedSalary.$dirty && valid3.expectedSalary.$invalid">
                                                        <span ng-show="valid3.expectedSalary.$error.required">Expected
                                                            Salary
                                                            per Month is
                                                            Required .</span>
                                                    </span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="col-md-12">
                                                <label for="Phone">Would you work under
                                                    MGNREGA<span class="star">*</span>:</label>
                                            </div>

                                            <div class="col-md-12">
                                                <select class="form-control" name=" underMGNREGA"
                                                    ng-model="$parent.underMGNREGA" required>
                                                    <option value="" disabled selected>--select--</option>
                                                    <option value="Yes"> Yes</option>
                                                    <option value="No">No</option>
                                                </select>
                                            </div>

                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class="col-md-6" style="text-align: left;">
                                            <button class="btn btn-success " ng-click="previous2();"
                                                style="text-align: right;"><i class="fa fa-arrow-left" aria-hidden="true"></i> Back</button>
                                        </div>

                                        <div class="col-md-6" style="text-align: right;">
                                            <button type="button" class="btn btn-success" ng-click="next3();"ng-disabled="valid3.$invalid" >
                                                Next <i class="fa fa-arrow-right" aria-hidden="true"></i></button>
                                            <!-- ng-disabled="valid3.$invalid" -->
                                        </div>
                                    </div>


                                </div>
                            </form>

                        </div>
                        <div class="card card-primary" ng-show="part4 ">
                            <div class="card-header">
                                <h3 class="card-title">Skill and employment details</h3>
                            </div>
                            <form name="valid4" id="valid4"  >
                                <div class="card-body">
                                    <style>
