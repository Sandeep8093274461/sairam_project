<%- include('common/header.ejs') -%>
<div class="content-wrapper">

    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-1">
                <div class="col-sm-7">
                    <h1 class="m-0 text-dark"><img src="/img/icon/logo.jpg" style="width:60px;height:60px;" alt=""> Sai
                        Sabujeema(Plantation) </h1>
                </div><!-- /.col -->
                <div class="col-sm-5">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Sai Sabujeema</li>
                        <li class="breadcrumb-item active">Details</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">


        <div class="container-fluid">
            <div class="container-fluid">
                <div>
                    <form name="bloodCamp">
                        <div class="card card-primary" id="applicantDetails" ng-show="alldata.length != 0">

                            <style type="text/css" media="print">
                                @media print {
                                    @page {
                                        size: A4 portrait;
                                        max-height: 100%;
                                        max-width: 100%;
                                        zoom: 0.1%;
                                        margin: 0;
                                        size: auto;
                                    }

                                    body {
                                        width: 100%;
                                        height: 100%;
                                        transform: scale(0.9);
                                    }
                                }
                            </style>
                            <style>
                                #rcorners2 {
                                    border-radius: 25px;
                                    border: 2px solid rgb(78, 88, 59);
                                    padding: 20px;
                                    width: 70px;
                                    /* height: 51px; */
                                    margin-left: 20%;
                                }
                            </style>
                            <style>
                                .table-responsive {
                                    height: 600px;
                                }

                                .panel {
                                    margin-bottom: 20px;
                                    border-radius: 7px;
                                }
                            </style>
                            <script type="text/javascript" src="/js/angular.min.js"></script>
                            <link rel="stylesheet" href="/AdminLTE/adminlte.min.css">
                            <div class="card-header">
                                <h4>Sai Sabujeema details</h4>
                            </div>

                            <div class="card-body" style="padding: 0;">
                                <div style="height: auto;padding-bottom: 25px; "><br>

                                    <div class="row">

                                        <div class="col-lg-10 col-md-8 col-sm-8 col-xs-8">
                                            <div class="row">

                                                <div class="col-md-3 col-xs-3">
                                                    &nbsp District:
                                                </div>
                                                <div class="col-md-2 col-xs-2">
                                                    <h6>{{alldata.dist_name}}</h6>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    &nbsp; No.of plantation at Individual Household:
                                                </div>

                                                <div class="col-md-1 col-xs-1">
                                                    <h6 style="margin-left: -45px;"  >{{alldata.noatindividualhousehold}}
                                                    </h6>
                                                </div>
                                                <div class="col-md-3 col-xs-3">
                                                    &nbsp Samiti:
                                                </div>
                                                <div class="col-md-2 col-xs-3">
                                                    <h6>{{alldata.samithi_name}}</h6>
                                                </div>

                                                <div class=" col-md-6 col-xs-6">
                                                    &nbsp; No.of plantation at community places:
                                                </div>
                                                <div class="col-md-1 col-xs-1">
                                                    <h6 style="margin-left: -45px;"  >{{alldata.noatcommunityplaces}}</h6>
                                                </div>
                                                <div class="col-md-3 col-xs-3">
                                                    &nbsp Bhajan Mandali :
                                                </div>
                                                <div class="col-md-2 col-xs-2"
                                                ng-show="alldata.bhajanmandali_id != null  && alldata.bhajanmandali_name != null">
                                                    <h6>{{alldata.bhajanmandali_name}}</h6>
                                                </div>
                                                <div class="col-md-2 col-xs-2"
                                                ng-show="alldata.bhajanmandali_id != null  && alldata.bhajanmandali_name == null">
                                                    <h6>{{alldata.samithi_name}}</h6>
                                                </div>
                                                <div class="col-md-2 col-xs-2"
                                                ng-show="alldata.bhajanmandali_id == null && alldata.bhajanmandali_name == null">
                                                    <h6>{{alldata.bhajanmandali_name || '--'}}</h6>
                                                </div>
                                                <div class=" col-md-6 col-xs-6">
                                                    &nbsp No.of plantation at SSSVIP :
                                                </div>
                                                <div class="col-md-1 col-xs-1">
                                                    <h6  style="margin-left: -45px;" >{{alldata.noatssvip}}</h6>
                                                </div>


                                                <div class="col-md-3 col-xs-3">
                                                    &nbsp Date of report:
                                                </div>

                                                <div class="col-md-2 col-xs-2">
                                                    <h6>
                                                        {{alldata.monthended | date:'dd-MM-yyyy'}}
                                                    </h6>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    &nbsp No. of previous year plants Sustain/ maintain:
                                                </div>
                                                <div class="col-md-1 col-xs-1">
                                                    <h6 style="margin-left: -45px;" >{{alldata.nopreviousyearplant}}</h6>
                                                </div>




                                                <div class="col-md-3 col-xs-3">
                                                    &nbsp No.of Tulshi Plant:
                                                </div>
                                                <div class="col-md-2 col-xs-2">
                                                    <h6>{{alldata.notulshiplant}}</h6>
                                                </div>
                                                <div class=" col-md-6 col-xs-6">
                                                    &nbsp No.of Neem Plant:
                                                </div>

                                                <div class="col-md-1 col-xs-1">
                                                    <h6  style="margin-left: -45px;">{{alldata.noneemplant}}</h6>
                                                </div>
                                                <div class="col-md-3 col-xs-3">
                                                    &nbsp No.of Sajana Plant :
                                                </div>
                                                <div class="col-md-2 col-xs-2">
                                                    <h6>{{alldata.nosajanplant}}</h6>
                                                </div>
                                                <div class=" col-md-6 col-xs-6">
                                                    &nbsp Others, if any:
                                                </div>
                                                <div class="col-md-1 col-xs-1">
                                                    <h6 style="margin-left: -45px;" >{{alldata.noothersplant}}</h6>
                                                </div>
                                                <div class="col-md-3 col-xs-3">
                                                    &nbsp Remark:
                                                </div>
                                                <div class="col-md-8 col-xs-8">
                                                    <h6>{{alldata.remarks}}</h6>
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                            <div class="row">
                                              
                                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                                    <img style=" height: 150px;width: 150px; "
                                                        ng-src="{{alldata.photo}}" />
                                                </div>


                                            </div>
                                        </div>

                                    </div>

                                    <!-- <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                           
                                        <img style="margin-left: 320px;margin-top: 50px; height: 140px;width: 140px; "
                                            ng-src="{{alldata.photo}}" />
                                    </div> -->




                                </div>
                            </div>


                        </div>
                    </form>
                </div>
                <div class="row">
                    <div class="col-md-6" style="text-align: left; padding-bottom: 100px;">
                        <button class="btn btn-success" ng-click="back(alldata.register);" style="text-align: right;"><i
                                class="fa fa-arrow-left" aria-hidden="true"></i> Back</button>
                    </div>
                    <div class="col-md-6" style="text-align: right;">
                        <button class="btn btn-success" onclick="printElem()" style="text-align: right;"><i
                                class="fa fa-print" aria-hidden="true"></i> Print</button>
                    </div>

                </div>
            </div>
        </div>

    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
<footer class="main-footer">

    <div class="float-right d-none d-sm-inline-block">
        <b>Version</b> 1.0.0-pre
    </div>
</footer>

<!-- Control Sidebar -->
<aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
</aside>
<!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->
<!-- Controller -->
<script>
    var sabujeema_id = "<%= sabujeema_id %>";
</script>
<script src="/controller/admin/saiSabujeemaViewPage.js"></script>
<!-- AdminLTE App -->
<script src="/AdminLTE/adminlte.js"></script>
<script type="text/javascript" src="/AdminLTE/bootstrap.bundle.min.js"></script>

</body>

</html>