<!-- detailsDonor -->
<%- include('common/header.ejs') -%>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0 text-dark"><img src="/img/icon/blood.png" style="width:60px;height:60px;" alt="">
                        Donor Details</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Blood Group</li>
                        <li class="breadcrumb-item active">Donor Details</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">


        <div class="container-fluid">
            <div class="container-fluid">
                <div class="panel panel-success">
                    <form name="bloodCamp">

                        <div class="card card-primary">
                            <div class="card-header">
                                <h3 class="card-title">Donor Details</h3>
                            </div>
                            <div ng-show="alldata.length == 0" style="text-align: center; padding-bottom: 30px;">
                                <br>
                                <label for="" style="color: red;"><b>No record found</b></label>
                            </div>
                            <div class="card-body" style="padding: 0;" ng-show="alldata.length != 0">
                                <div class="table-responsive" style="height: auto">
                                    <table class="table table-head-fixed" id="allListTable">

                                        <thead>
                                            <tr ng-hide="results.length == 0">
                                                <th>sl.</th>
                                                <th>Doner id</th>
                                                <th>Name</th>
                                                <th>Gender</th>
                                                <th>Date of Birth</th>
                                                <th>Blood Group</th>
                                                <th ng-if="data.lastdonationdate != null">Last Donated</th>
                                                <th ng-if="data.lastdonationdate == null">Last Donated</th>
                                                <th>District</th>
                                                <th>Address</th>
                                                <th>View</th>
                                                <th>Edit</th>
                                                <th>Delete</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr ng-repeat="data in results =  (indexDonorList | orderBy:'district')">

                                                <div ng-show="results.length == 0" style="text-align: center;">
                                                    <br>
                                                    No record found
                                                </div>
                                                <div ng-show="results.length != 0" style="text-align: center;">
                                                    <td> {{ $index+1 }}</td>
                                                    <td> {{ data.donerid }} </td>
                                                    <td> {{ data.name }} </td>
                                                    <td> {{ data.gender }}</td>
                                                    <td> {{ data.dob| date:'dd-MM-yyyy' }}</td>
                                                    <td> {{ data.bg }} </td>
                                                    <td ng-if="data.lastdonationdate != null">
                                                        {{ data.lastdonationdate | date:'dd-MM-yyyy' || '--'}}
                                                    </td>
                                                    <td ng-if="data.lastdonationdate == null">
                                                        {{'--'}}
                                                    </td>
                                                    <td> {{ data.district }}</td>
                                                    <td> {{ data.address }}</td>
                                                    <td>
                                                        <button class="btn btn-outline-success" data-toggle="modal"
                                                            data-target="#exampleModal1" ng-click="view(data);">
                                                            <i class="fas fa-eye" aria-hidden="true"></i>
                                                        </button>
                                                    </td>
                                                    <td>
                                                        <button class="btn btn-outline-primary"
                                                            ng-click="editField(data.donerid);">
                                                            <i class="fas fa-edit" aria-hidden="true"></i>
                                                        </button>
                                                    </td>
                                                    <td>
                                                        <button class="btn btn-outline-danger" data-toggle="modal"
                                                            data-target="#exampleModal" ng-click="deleted(data);">
                                                            <i class="fas fa-trash" aria-hidden="true"></i>
                                                        </button>
                                                    </td>

                                                </div>
                                                <div class="modal fade" id="exampleModal1" tabindex="-1" role="dialog"
                                                    aria-labelledby="exampleModalLabel" aria-hidden="true">
                                                    <div class="modal-dialog" role="document">
                                                        <div class="modal-content">
                                                            <div class="modal-header" style="background-color:#28a745;">
                                                                <h5 class="modal-title" id="exampleModalLabel"
                                                                    style="padding-left: 170px;">
                                                                    Donor Details </h5>
                                                                <button type="button" class="close" data-dismiss="modal"
                                                                    aria-label="Close">
                                                                    <span aria-hidden="true">&times;</span>
                                                                </button>
                                                            </div>
                                                            <div class="modal-body">

                                                                <div class="row">
                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Doner id:
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>{{allDetails.donerid}}</h6>
                                                                    </div>
                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Name:
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>{{allDetails.name}}</h6>
                                                                    </div>

                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Gender:
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>{{allDetails.gender}}</h6>
                                                                    </div>


                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Date of Birth:
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>{{allDetails.dob | date:'dd-MM-yyyy'}}</h6>
                                                                    </div>



                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Blood Group:
                                                                    </div>

                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>
                                                                            {{allDetails.bg }}
                                                                        </h6>
                                                                    </div>
                                                                    

                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Last donation date:
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8"
                                                                        ng-if="data.lastdonationdate != ''">
                                                                        <h6>{{allDetails.lastdonationdate | date:'dd-MM-yyyy' }}
                                                                        </h6>
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8"
                                                                        ng-if="data.lastdonationdate == ''">
                                                                        <h6> {{'--'}}</h6>
                                                                    </div>

                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Contact no. :
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>{{allDetails.contactno}}</h6>
                                                                    </div>

                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Person Ref. :
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>{{allDetails.contactref || '--'}}</h6>
                                                                    </div>
                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp District :
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>{{allDetails.district}}</h6>
                                                                    </div>
                                                                    <div class="col-md-4 col-xs-4">
                                                                        &nbsp Address :
                                                                    </div>
                                                                    <div class="col-md-8 col-xs-8">
                                                                        <h6>{{allDetails.address}}</h6>
                                                                    </div>

                                                                </div>





                                                            </div>
                                                            <div class="modal-footer">
                                                                <button type="button" class="btn btn-success"
                                                                    data-dismiss="modal">Ok</button>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog"
                                                    aria-labelledby="exampleModalLabel" aria-hidden="true">
                                                    <div class="modal-dialog" role="document">
                                                        <div class="modal-content">
                                                            <div class="modal-header"
                                                                style="background-color: rgb(252, 62, 62);">
                                                                <h5 class="modal-title" id="exampleModalLabel">
                                                                    Confirm</h5>
                                                                <button type="button" class="close" data-dismiss="modal"
                                                                    aria-label="Close">
                                                                    <span aria-hidden="true">&times;</span>
                                                                </button>
                                                            </div>
                                                            <div class="modal-body">
                                                                Are you sure want to delete
                                                                <strong>{{allDetails.name}}</strong> ?
                                                            </div>
                                                            <div class="modal-footer">
                                                                <button type="button" class="btn btn-secondary"
                                                                    data-dismiss="modal">No</button>
                                                                <button type="button" class="btn btn-danger"
                                                                    data-dismiss="modal"
                                                                    ng-click="deleteField(allDetails.donerid);">Yes</button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </tr>

                                        </tbody>

                                    </table>
                                </div>
                                <div style="text-align: right;margin: 10px;">
                                    <button type="button" class="btn btn-primary"
                                        ng-click="exportToExcel('#allListTable')" ng-show="results.length != 0"> <i
                                            class="fas fa-download" aria-hidden="true"></i> Download</button>
                                </div>
                            </div>


                        </div>
                    </form>
