<%- include('common/header.ejs') -%>
<div class="content-wrapper">

    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-1">
                <div class="col-sm-7">
                    <h1 class="m-0 text-dark"><img src="/img/icon/logo.jpg" style="width:60px;height:60px;" alt=""> Sai
                        Swabalambi Activities Report </h1>
                </div><!-- /.col -->
                <div class="col-sm-5">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Sai Swabalambi Program</a></li>
                        <li class="breadcrumb-item active">Activities Report</li>
                        <li class="breadcrumb-item active">Details</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">


        <div class="container-fluid">
            <div class="container-fluid">
                <div>
                    <form name="bloodCamp">
                        <div class="card card-primary" id="applicantDetails" ng-show="alldata.length != 0">

                            <style type="text/css" media="print">
                                @media print {
                                    @page {
                                        size: A4 portrait;
                                        max-height: 100%;
                                        max-width: 100%;
                                        zoom: 0.1%;
                                        margin: 0;
                                        size: auto;
                                    }

                                    body {
                                        width: 100%;
                                        height: 100%;
                                        transform: scale(0.9);
                                    }
                                }
                            </style>
                            <style>
                                #rcorners2 {
                                    border-radius: 25px;
                                    border: 2px solid rgb(78, 88, 59);
                                    padding: 20px;
                                    width: 70px;
                                    /* height: 51px; */
                                    margin-left: 20%;
                                }
                            </style>
                            <style>
                                .table-responsive {
                                    height: 600px;
                                }

                                .panel {
                                    margin-bottom: 20px;
                                    border-radius: 7px;
                                }
                            </style>
                            <script type="text/javascript" src="/js/angular.min.js"></script>
                            <link rel="stylesheet" href="/AdminLTE/adminlte.min.css">
                            <div class="card-header">
                                <h4>Activities report details</h4>
                            </div>

                            <div class="card-body" style="padding: 0;">
                                <div style="height: auto;padding-bottom: 25px; width: auto; "><br>

                                    <div class="row">

                                        <div class="col-lg-10 col-md-10 col-sm-10 col-xs-10">
                                            <div class="row">
                                                <div class=" col-md-3 col-xs-3">
                                                    &nbsp; Name of Resource Person:
                                                </div>
                                                
                                                <div class="col-md-8 col-xs-8">
                                                    <h6>{{alldata.nameofresourceperson}}</h6>
                                                </div>
                                                <div class="col-md-12 col-xs-12"></div>
                                                <div class="col-md-2 col-xs-2">
                                                    &nbsp; Date of activity:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.doa| date:'dd-MM-yyyy'}}
                                                    </h6>
                                                </div>
                                                <div class="col-md-3 col-xs-3">
                                                    No. of Participants:
                                                </div>
                                                <div class="col-md-3 col-xs-3">
                                                    <h6>{{alldata.participants}}</h6>
                                                </div>

                                                <div class="col-md-2 col-xs-2">
                                                    &nbsp; District:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.dist_name}}</h6>
                                                </div>
                                                <div class="col-md-3 col-xs-3">
                                                 Name of the Person who will:
                                                </div>
                                                <div class="col-md-3 col-xs-3">
                                                    <h6>{{alldata.nameofresourceperson}}</h6>
                                                </div>

                                                <div class="col-md-2 col-xs-2">
                                                    &nbsp; Samiti:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.samithi_name}}</h6>
                                                </div>
                                               
                                                <div class=" col-md-3 col-xs-3">
                                                    Activity related to :
                                                </div>
                                                <div class="col-md-3 col-xs-2">
                                                    <h6>{{alldata.activityrelated}}</h6>
                                                </div>
                                                <div class="col-md-2 col-xs-2">
                                                    &nbsp; Type of Activity:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.type}}
                                                    </h6>
                                                </div>
                                                <div class=" col-md-3 col-xs-3">
                                                    Action taken details :
                                                </div>
                                                <div class="col-md-3 col-xs-2">
                                                    <h6>{{alldata.actiondetails}}</h6>
                                                </div>



                                                <div class="col-md-2 col-xs-2">
                                                    &nbsp; Remark:
                                                </div>
                                                <div class="col-md-8 col-xs-8">
                                                    <h6>{{alldata.remarks}}</h6>
                                                </div>

                                            </div>
                                        </div>
                                        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1">
                                            <div class="row">

                                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                                    <label for="">Photo1</label>
                                                    <img style=" height: 150px;width: 150px; "
                                                        ng-src="{{alldata.photo1url}}" />
                                                </div>
                                            </div>
                                            <div class="row">

                                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                                    <label for="">Photo2</label>
                                                    <img style=" height: 150px;width: 150px; "
                                                        ng-src="{{alldata.photo2url}}" />
                                                </div>



                                            </div>
                                        </div>

                                    </div>

                                    <!-- <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                           
                                        <img style="margin-left: 320px;margin-top: 50px; height: 140px;width: 140px; "
                                            ng-src="{{alldata.photo}}" />
                                    </div> -->




                                </div>
                            </div>


                        </div>
                    </form>
                </div>
                <div class="row">
                    <div class="col-md-6" style="text-align: left; padding-bottom: 100px;">
                        <button class="btn btn-success" ng-click="back(alldata.register);" style="text-align: right;"><i
                                class="fa fa-arrow-left" aria-hidden="true"></i> Back</button>
                    </div>
                    <div class="col-md-6" style="text-align: right;">
                        <button class="btn btn-success" onclick="printElem()" style="text-align: right;"><i
                                class="fa fa-print" aria-hidden="true"></i> Print</button>
                    </div>

                </div>
            </div>
        </div>

    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
<footer class="main-footer">

    <div class="float-right d-none d-sm-inline-block">
        <b>Version</b> 1.0.0-pre
    </div>
</footer>

<!-- Control Sidebar -->
<aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
</aside>
<!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->
<!-- Controller -->
<script>
    var activitiesreportid = "<%= activitiesreportid %>";
</script>
<script src="/controller/admin/activitiesReportViewPage.js"></script>
<!-- AdminLTE App -->
<script src="/AdminLTE/adminlte.js"></script>
<script type="text/javascript" src="/AdminLTE/bootstrap.bundle.min.js"></script>

</body>

</html>