<%- include('common/header.ejs') -%>
<div class="content-wrapper">

    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-1">
                <div class="col-sm-8">
                    <h1 class="m-0 text-dark"><img src="/img/icon/logo.jpg" style="width:60px;height:60px;" alt=""> Sai Swabalambi Office Bearers Information
                    </h1>
                </div><!-- /.col -->
                <div class="col-sm-4">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Office Bearers</li>
                        <li class="breadcrumb-item active">Details</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <!-- Main content -->
    <section class="content">


        <div class="container-fluid">
            <div class="container-fluid">
                <div>
                    <form name="bloodCamp">
                        <div class="card card-primary" id="applicantDetails" ng-show="alldata.length != 0">

                            <style type="text/css" media="print">
                                @media print {
                                    @page {
                                        size: A4 portrait;
                                        max-height: 100%;
                                        max-width: 100%;
                                        zoom: 0.1%;
                                        margin: 0;
                                        size: auto;
                                    }

                                    body {
                                        width: 100%;
                                        height: 100%;
                                        transform: scale(0.9);
                                    }
                                }
                            </style>
                            <style>
                                #rcorners2 {
                                    border-radius: 25px;
                                    border: 2px solid rgb(78, 88, 59);
                                    padding: 20px;
                                    width: 70px;
                                    /* height: 51px; */
                                    margin-left: 20%;
                                }
                            </style>
                            <style>
                                .table-responsive {
                                    height: 600px;
                                }

                                .panel {
                                    margin-bottom: 20px;
                                    border-radius: 7px;
                                }
                            </style>
                            <script type="text/javascript" src="/js/angular.min.js"></script>
                            <link rel="stylesheet" href="/AdminLTE/adminlte.min.css">
                            <div class="card-header">
                                <h4>Office Bearers Information details</h4>
                            </div>

                            <div class="card-body" style="padding: 0;">
                                <div style="height: auto;padding-bottom: 25px; "><br>

                                    <div class="row">
                                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                            <div class="row">
                                                <div class="col-md-5 col-xs-5">
                                                    &nbsp; Level:
                                                </div>
                                                <div class="col-md-7 col-xs-7">
                                                    <h6>{{alldata.lavel_name}}</h6>
                                                </div>
                                                                                  
                                                <div class="col-md-5 col-xs-5" ng-show="alldata.lavel_name != 'State' || alldata.lavel_name == 'District'">
                                                    &nbsp; District :
                                                </div>
                                                <div class="col-md-7 col-xs-7" ng-show="alldata.lavel_name != 'State'  || alldata.lavel_name == 'District'">
                                                    <h6>{{alldata.dist_name}}</h6>
                                                </div>
                                                <div class="col-md-5 col-xs-5" ng-show="alldata.lavel_name == 'Samiti' || alldata.lavel_name == 'BM'">
                                                    &nbsp; Samiti :
                                                </div>
                                                <div class="col-md-7 col-xs-7" ng-show="alldata.lavel_name == 'Samiti' || alldata.lavel_name == 'BM'">
                                                    <h6>{{alldata.samithi_name}}</h6>
                                                </div>
                                                <div class="col-md-5 col-xs-5" ng-show=" alldata.lavel_name == 'BM'">
                                                    &nbsp; Bhajan Mandali:
                                                </div>
                                                <div class="col-md-7 col-xs-7"
                                                ng-show="alldata.bhajanmandali_id != null  && alldata.bhajanmandali_name != null && alldata.lavel_name == 'BM' ">
                                                    <h6>{{alldata.bhajanmandali_name}}</h6>
                                                </div>
                                                <div class="col-md-7 col-xs-7"
                                                ng-show="alldata.bhajanmandali_id == null  && alldata.bhajanmandali_name == null  && alldata.lavel_name == 'BM' ">
                                                    <h6>{{alldata.samithi_name}}</h6>
                                                </div>
                                               


                                                <div class="col-md-5 col-xs-5">
                                                    &nbsp; Designation :
                                                </div>
                                                <div class="col-md-7 col-xs-7">
                                                    <h6>{{alldata.designation_name}}</h6>
                                                </div>
                                                <div class="col-md-5 col-xs-5">
                                                    &nbsp; Name:
                                                </div>
                                                <div class="col-md-7 col-xs-7">
                                                    <h6>{{alldata.nameofbearers}}</h6>
                                                </div>
                                                <div class="col-md-5 col-xs-5">
                                                    &nbsp; Age:
                                                </div>
                                                <div class="col-md-7 col-xs-7">
                                                    <h6>{{alldata.age}}</h6>
                                                </div>
                                                <div class="col-md-5 col-xs-5">
                                                    &nbsp; Qualification :
                                                </div>
                                                <div class="col-md-7 col-xs-7">
                                                    <h6>{{alldata.qualification}}</h6>
                                                </div>
                                                <div class="col-md-5 col-xs-5">
                                                    &nbsp; Profession:
                                                </div>
                                                <div class="col-md-7 col-xs-7">
                                                    <h6>{{alldata.profession}}</h6>
                                                </div>
                                                <div class="col-md-5 col-xs-5">
                                                    &nbsp; Sai One ID:
                                                </div>
                                                <div class="col-md-7 col-xs-7">
                                                    <h6>{{alldata.saiid}}</h6>
                                                </div>
                                                <div class="col-md-5 col-xs-5">
                                                    &nbsp; Email:
                                                </div>
                                                <div class="col-md-7 col-xs-7">
                                                    <h6>{{alldata.email}}</h6>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                            <div class="row">
                                                <div class="col-md-8 col-xs-8">
                                                    &nbsp; Mobile number:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.mobno}}</h6>
                                                </div>
                                                <div class="col-md-8 col-xs-8">
                                                    &nbsp; Whatsapp number:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.wpno}}</h6>
                                                </div>
                                                
                                                
                                                <div class="col-md-8 col-xs-8">
                                                    &nbsp; Year of joining organisation:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.yearofjoining}}</h6>
                                                </div>
                                                <div class="col-md-8 col-xs-8">
                                                    &nbsp; How many year you have been in this post:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.howmanyyearofjoin}}</h6>
                                                </div>
                                                <div class="col-md-8 col-xs-8">
                                                    &nbsp; Previous responsibility:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.responsibility}}</h6>
                                                </div>
                                                <div class="col-md-8 col-xs-8">
                                                    &nbsp; Are you Bv Alumni:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.alumni}}</h6>
                                                </div>
                                                <div class="col-md-8 col-xs-8">
                                                    &nbsp; When can u join Prasanthi Seva:
                                                </div>
                                                <div class="col-md-4 col-xs-4">
                                                    <h6>{{alldata.joinprasanthiseva}}</h6>
                                                </div>

                                            </div>
                                        </div>
                                      
                                        <div class="col-lg-2 col-md-2 col-sm-2 col-xs-2">
                                            <div class="row">
                                              
                                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
                                                    <img style=" height: 150px;width: 150px; "
                                                        ng-src="{{alldata.passportphoto}}" />
                                                </div>


                                            </div>
                                        </div>

                                    </div>
                                    <!-- <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                           
                                        <img style="margin-left: 320px;margin-top: 50px; height: 140px;width: 140px; "
                                            ng-src="{{alldata.photo}}" />
                                    </div> -->




                                </div>
                            </div>


                        </div>
                    </form>
                </div>
                <div class="row">
                    <div class="col-md-6" style="text-align: left; padding-bottom: 100px;">
                        <button class="btn btn-success" ng-click="back(alldata.register);" style="text-align: right;"><i
                                class="fa fa-arrow-left" aria-hidden="true"></i> Back</button>
                    </div>
                    <div class="col-md-6" style="text-align: right;">
                        <button class="btn btn-success" onclick="printElem()" style="text-align: right;"><i
                                class="fa fa-print" aria-hidden="true"></i> Print</button>
                    </div>

                </div>
            </div>
        </div>

    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
<footer class="main-footer">

    <div class="float-right d-none d-sm-inline-block">
        <b>Version</b> 1.0.0-pre
    </div>
</footer>

<!-- Control Sidebar -->
<aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
</aside>
<!-- /.control-sidebar -->
</div>
<!-- ./wrapper -->
<!-- Controller -->
<script>
    var officebearersinformationid = "<%= officebearersinformationid %>";
</script>
<script src="/controller/admin/officeBearersInformationViewPage.js"></script>
<!-- AdminLTE App -->
<script src="/AdminLTE/adminlte.js"></script>
<script type="text/javascript" src="/AdminLTE/bootstrap.bundle.min.js"></script>

</body>

</html>